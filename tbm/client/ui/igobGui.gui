function getlinecount(%filename) {
	%file = new FileObject();
	%file.openForRead(%filename);
	while(!%file.isEOF())
	{
	%file.readLine();
    %linenum++;
    }
    return %linenum;
	%file.close();
	%file.delete();
}
function LoadiGobList()
{
iGoblist.clear();
%loc = "tbm/client/iGob/*.save";
%rowcount = 0;
for (%filename = findFirstFile(%loc); %filename !$= ""; %filename = findNextFile(%loc)) {
  if(strstr(%filename,"iGob_temp") != -1)
    continue;
  if((%brickcount = getlinecount(%filename)) > 0) {
    %filename = strreplace(strreplace(%filename,".save",""),"tbm/client/iGob/","");
    iGoblist.addrow(%rowcount++, %filename TAB %brickcount);
  }
}
iGoblist.sort(0);
}

