function deleteiGob() {
if(iGoblist.getRowTextbyID(iGoblist.getSelectedID()) !$= "") {
  %file = new FileObject();
  %file.openForWrite("tbm/client/iGob/" @ getField(iGoblist.getRowTextbyID(iGoblist.getSelectedID()), 0) @ ".save");
  %file.delete();
  iGoblist.removeRowByID(iGoblist.getSelectedID());
}
}

