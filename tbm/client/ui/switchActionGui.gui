function SwitchActionGui::onWake(%this) {
SwitchActionWindow.setText($Client::Switch::Realname[$Client::Switch::Type[switchActionGui.currentAction]]);
}

function SwitchAction_OK() {
$SwitchActionFrame[$Client::Switch::Type[switchActionGui.currentAction]].save(switchActionGui.currentAction);
$Client::Switch::Sleep[switchActionGui.currentAction] = Switch_Sleep.getValue();
if(switchActionGui.currentAction == $Client::Switch::numActions + 1)
  $Client::Switch::numActions = switchActionGui.currentAction;
canvas.popDialog(SwitchActionGui);
loadSwitchActionList();
}

function SwitchAction_Cancel() {
canvas.popDialog(SwitchActionGui);
if(switchActionGui.currentAction > $Client::Switch::numActions)
  $Client::Switch::Type[switchActionGui.currentAction] = ""; //We should be okay if this stays as whatever since nothing over the maximum ever gets cleaned up, but it can't hurt to have this here.
}

function Effects_popupmenuOnSelect() {
%text = Effects_popupmenu.getText();
FX_Image.setValue(%text);
}

