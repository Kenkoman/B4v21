function Frame_Switch_Turret::populate(%this, %action) {
      nameToID(Turret_Name).settext($Client::Switch::Doorset[%action]);
      nameToID(Turret_Image).settext($Client::Switch::Direction[%action]);
      nameToID(Turret_Delay).settext(getWord($Client::Switch::Delay[%action], 0));
      nameToID(Turret_Range).settext(getWord($Client::Switch::Delay[%action], 1));
      nameToID(Turret_Targets).clear();
      for(%i = 1; %i <= $Client::Switch::numGroups; %i++)
        nameToID(Turret_Targets).add($Client::Switch::Groupname[$Client::Switch::Group[%i]], %i);
      nameToID(Turret_Targets).setSelected(1);
      for(%i = 1; %i <= $Client::Switch::numGroups; %i++) {
        if($Client::Switch::Times[%action] $= $Client::Switch::Group[%i])
          nameToID(Turret_Targets).setSelected(%i);
      }
}

