//Save methods, to save the action's appropriate data back into the data arrays

function Frame_Switch_Teleporter::save(%this, %action) {
    $Client::Switch::Delay[%action]= "";
    $Client::Switch::Direction[%action]= $Client::Switch::TeleportDestination[%action];
    $Client::Switch::Doorset[%action] = -2;
    $Client::Switch::Times[%action] = "";
    //Nothing atm really; 2.2 teleporters don't have any options to speak of.
    //2.3 will hopefully have some effects to choose from.
}

function Frame_Switch_Jumper::save(%this, %action) {
    $Client::Switch::Delay[%action]= nameToID(Jumper_Delay).getValue();
    $Client::Switch::Direction[%action] = mRound(nameToID(Jumper_X).getValue()) SPC mRound(nameToID(Jumper_Y).getValue()) SPC mRound(nameToID(Jumper_Z).getValue());
    $Client::Switch::Doorset[%action] =-1;
    $Client::Switch::Times[%action] = nameToID(Jumper_Times).getValue();
}

function Frame_Switch_Mover::save(%this, %action) {
    $Client::Switch::Delay[%action]= mRound(nameToID(Mover_Speed).getValue()) * 100;
    $Client::Switch::Times[%action] = mRound(nameToID(Mover_Smooth).getValue());
    $Client::Switch::Doorset[%action] = nameToID(Mover_Name).getValue();
    $Client::Switch::Direction[%action] =(nameToID(Mover_X).getValue() / 2 / $Client::Switch::Times[%action]) SPC (nameToID(Mover_Y).getValue() / 2 / $Client::Switch::Times[%action]) SPC (nameToID(Mover_Z).getValue() / 5 / $Client::Switch::Times[%action]);
    $Client::Switch::Delay[%action]= mRound($Client::Switch::Delay[%action] / $Client::Switch::Times[%action]);
}

function Frame_Switch_Rotator::save(%this, %action) {
    $Client::Switch::Delay[%action]= mRound(nameToID(Rotator_Speed).getValue()) * 100;
    $Client::Switch::Times[%action] = mRound(nameToID(Rotator_Smooth).getValue());
    $Client::Switch::Doorset[%action] = nameToID(Rotator_Name).getValue();
    $Client::Switch::Direction[%action] =(nameToID(Rotator_X).getValue() / $Client::Switch::Times[%action]) SPC (nameToID(Rotator_Y).getValue() / $Client::Switch::Times[%action]) SPC (nameToID(Rotator_Z).getValue() / $Client::Switch::Times[%action]);
    $Client::Switch::Delay[%action]= mRound($Client::Switch::Delay[%action] / $Client::Switch::Times[%action]);
}

function Frame_Switch_MR::save(%this, %action) {
    $Client::Switch::Delay[%action]= mRound(nameToID(MR_Speed).getValue()) * 100;
    $Client::Switch::Times[%action] = mRound(nameToID(MR_Smooth).getValue());
    $Client::Switch::Doorset[%action] = nameToID(MR_Name).getValue();
    $Client::Switch::Direction[%action] =(nameToID(MRM_X).getValue() / 2 / $Client::Switch::Times[%action]) SPC (nameToID(MRM_Y).getValue() / 2 / $Client::Switch::Times[%action]) SPC (nameToID(MRM_Z).getValue() / 5 / $Client::Switch::Times[%action]);
    $Client::Switch::Direction[%action] =$Client::Switch::Direction[%action] SPC (nameToID(MRR_X).getValue() / $Client::Switch::Times[%action]) SPC (nameToID(MRR_Y).getValue() / $Client::Switch::Times[%action]) SPC (nameToID(MRR_Z).getValue() / $Client::Switch::Times[%action]);    
    $Client::Switch::Delay[%action]= mRound($Client::Switch::Delay[%action] / $Client::Switch::Times[%action]);
}

function Frame_Switch_Cloaker::save(%this, %action) {
