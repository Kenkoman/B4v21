    $Client::Switch::Doorset[%action] = nameToID(Scaler_Name).getValue();
    $Client::Switch::Direction[%action] =(nameToID(Scaler_X).getValue() / $Client::Switch::Times[%action]) SPC (nameToID(Scaler_Y).getValue() / $Client::Switch::Times[%action]) SPC (nameToID(Scaler_Z).getValue() / $Client::Switch::Times[%action]);
    $Client::Switch::Delay[%action]= mRound($Client::Switch::Delay[%action] / $Client::Switch::Times[%action]);
